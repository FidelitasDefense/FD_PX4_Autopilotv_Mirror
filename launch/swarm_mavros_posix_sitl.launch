        <include file="$(find px4)/launch/mavros.launch">
            <arg name="ns" value="$(arg ns)"/>
            <arg name="gcs_url" value="$(arg gcs_url)"/> <!-- GCS link is provided by SITL -->
            <arg name="tgt_component" value="$(arg tgt_component)" />
