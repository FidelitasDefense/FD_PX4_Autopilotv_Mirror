        <node name="$(anon vehicle_spawn2)" output="screen" pkg="gazebo_ros" type="spawn_model"
            args="-sdf -file $(arg sdf) -model $(arg model) -x $(arg x) -y -2 -z $(arg z) -R $(arg R) -P $(arg P) -Y $(arg Y)"/>

        <include file="$(find px4)/launch/mavros.launch">
