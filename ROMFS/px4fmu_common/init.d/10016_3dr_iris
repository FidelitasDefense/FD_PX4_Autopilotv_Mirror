#!nsh
#
# @name 3DR Iris Quadrotor
#
# @type Quadrotor Wide
#
# @maintainer Anton Babushkin <anton@px4.io>
#

sh /etc/init.d/rc.mc_defaults

if [ $AUTOCNF == yes ]
then
	# TODO tune roll/pitch separately
	param set MC_ROLL_P 7.0
	param set MC_ROLLR_P 0.15
	param set MC_ROLLR_I 0.05
	param set MC_ROLLR_D 0.004
	param set MC_PITCH_P 7.0
	param set MC_PITCHR_P 0.15
	param set MC_PITCHR_I 0.05
	param set MC_PITCHR_D 0.004
	param set MC_YAW_P 2.5
	param set MC_YAWR_P 0.25
	param set MC_YAWR_I 0.25
	param set MC_YAWR_D 0.0

	param set BAT_V_SCALING 0.00989
	param set BAT_C_SCALING 0.0124
fi

set MIXER quad_w

set PWM_OUT 1234
