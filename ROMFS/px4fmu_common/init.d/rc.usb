#!nsh
#
# USB MAVLink start
#

mavlink start -d /dev/ttyACM0 -m custom -r 20000

mavlink stream -d /dev/ttyACM0 -s OPTICAL_FLOW -r 10
usleep 100000
mavlink stream -d /dev/ttyACM0 -s ATTITUDE -r 50
usleep 100000
mavlink stream -d /dev/ttyACM0 -s LOCAL_POSITION_NED -r 50
usleep 100000

# Exit shell to make it available to MAVLink
exit