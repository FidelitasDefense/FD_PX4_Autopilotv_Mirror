#!nsh
#
# @name Team Blacksheep Discovery
#
# @type Quadrotor Wide
#
# @maintainer Anton Babushkin <anton@px4.io>, Simon Wilks <simon@px4.io>
#

sh /etc/init.d/rc.mc_defaults

if [ $AUTOCNF == yes ]
then
	param set MC_ROLL_P 6.5
	param set MC_ROLLR_P 0.1
	param set MC_ROLLR_I 0.05
	param set MC_ROLLR_D 0.0017
	param set MC_PITCH_P 6.5
	param set MC_PITCHR_P 0.14
	param set MC_PITCHR_I 0.1
	param set MC_PITCHR_D 0.0025
	param set MC_YAW_P 2.8
	param set MC_YAWR_P 0.28
	param set MC_YAWR_I 0.1
	param set MC_YAWR_D 0.0
fi

set MIXER quad_w

set PWM_OUT 1234
