#!nsh
#
# @name Team Blacksheep Discovery Endurance
#
# @type Quadrotor Wide
#
# @maintainer Simon Wilks <simon@px4.io>
#

sh /etc/init.d/rc.mc_defaults

if [ $AUTOCNF == yes ]
then
	param set BAT_N_CELLS 6
	param set BAT_V_EMPTY 3.5

	param set MC_ROLL_P 7.0
	param set MC_ROLLR_P 0.08
	param set MC_ROLLR_I 0.02
	param set MC_ROLLR_D 0.003
	param set MC_PITCH_P 7.0
	param set MC_PITCHR_P 0.13
	param set MC_PITCHR_I 0.02
	param set MC_PITCHR_D 0.005
	param set MC_YAW_P 2.8
	param set MC_YAWR_P 0.2
	param set MC_YAWR_I 0.1
	param set MC_YAWR_D 0.0

	param set MPC_XY_FF 0.2
	param set MPC_XY_VEL_MAX 2
fi

set MIXER quad_w

set PWM_OUT 1234
set PWM_MIN 1080
