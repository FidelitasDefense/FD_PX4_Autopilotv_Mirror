#!nsh
#
# @name F450-sized quadrotor with CAN
#
# @type Quadrotor x
#
# @maintainer Pavel Kirienko <pavel@px4.io>
#

sh /etc/init.d/4001_quad_x

if [ $AUTOCNF == yes ]
then
	param set MC_ROLL_P 7.0
	param set MC_ROLLR_P 0.16
	param set MC_ROLLR_I 0.05
	param set MC_ROLLR_D 0.003
	param set MC_PITCH_P 7.0
	param set MC_PITCHR_P 0.16
	param set MC_PITCHR_I 0.05
	param set MC_PITCHR_D 0.003
	param set MC_YAW_P 2.8
	param set MC_YAWR_P 0.3
	param set MC_YAWR_I 0.1
	param set MC_YAWR_D 0.0
fi

set OUTPUT_MODE uavcan_esc
